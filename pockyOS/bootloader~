slc = 3
x,y = term.getSize()
term.clear()
term.setCursorPos(x/2-2,y/2)
write("P")
sleep(0.4)
write("o")
sleep(0.4)
write("c")
sleep(0.4)
write("k")
sleep(0.4)
write("y")
sleep(2)
term.setCursorPos(x/2-1,y/2+2)
write("OS")
sleep(4)
term.clear()
term.setCursorPos(x/2-5,y/2)
write("Booting.")
sleep(0.2)
term.clear()
term.setCursorPos(x/2-5,y/2)
write("Booting..")
sleep(0.2)
